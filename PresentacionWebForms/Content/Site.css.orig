.container {
    padding-left: 0px;
}

@media (min-width: 1200px) {
    .container {
        max-width: none;
    }

    .col-lg-1,
    .col-lg-2,
    .col-lg-3,
    .col-lg-4,
    .col-lg-5,
    .col-lg-6,
    .col-lg-7,
    .col-lg-8,
    .col-lg-9,
    .col-lg-10,
    .col-lg-11 {
        float: left;
    }
}

.container {
    margin-left: 0px;
}

.content-wrapper,
.right-side {
    min-height: 100%;
    background-color: #ffffff;
    z-index: 800;
}

.table-responsive {
    width: 100%;
    height: 600px;
    overflow: scroll;
}

.borderless td, .borderless th {
    border: none;
}


#feedback {
    font-size: 1em;
}

.selectable .ui-selecting {
    background: #6a3da9;
}

.selectable .ui-selected {
    background: #522f83;
    color: white;
}

.selectable {
    list-style-type: none;
    margin: 0;
    padding: 0;
    height: 24em;
    overflow: scroll;
}

    .selectable li {
        font-size: 1em;
    }
